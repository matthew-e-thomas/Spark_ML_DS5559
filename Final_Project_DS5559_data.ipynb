    "#Find Outliers\n",
    "\n",
    "def outliers(df):\n",
    "    bnds = {}\n",
    "    cols = ['loan_amnt', 'int_rate', 'tot_cur_bal', 'total_pmnt', 'fico_range_low']\n",
    "\n",
    "    for col in cols:\n",
    "        quantiles = df.approxQuantile(col, [0.25, 0.75], 0.05)\n",
    "        IQR = quantiles[1] - quantiles[0]\n",
    "\n",
    "        bnds[col] = [\n",
    "         quantiles[0] - 1.5 * IQR,\n",
    "         quantiles[1] + 1.5 * IQR\n",
    "        ]\n",
    "    return bnds"
   ]
